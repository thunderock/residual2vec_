# fairwalk + dw
snakemake   train_features_2_vec      -call --config root=twitch_three gnn_model=gat env=local device=cuda:0 r2v=false crosswalk=false fairwalk=true node2vec=false dataset=twitch  --nolock --ignore-incomplete;

